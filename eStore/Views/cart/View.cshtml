@model List<ProductOrder>
    <div class="card-body m-3" >
        <table class="table">
            <thead>
            <tr>
                <th scope="col">Date</th>
                <th scope="col">Product</th>
                <th scope="col">Quantity</th>
                <th scope="col">Pricet</th>
                <th scope="col">isCheckedout</th>
            </tr>
            </thead>
            
            <tbody>
                @foreach(var ord in @Model)
                    {
                        <tr>
                            <td>@ord.Date</td>
                            <td>@ord.Product.Name</td>
                            <td>@ord.Quantity</td>
                            <td>@ord.Price</td>
                            <td>@ord.isCheckedout</td>
                        </tr>
                    }
            </tbody>
        </table>
        </div>
    
